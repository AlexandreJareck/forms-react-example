{
  "name": "forms-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "gcomponent": "yarn plop --plopfile generators/plopfile.js",
    "gtemplate": "yarn plop --plopfile generators/plopfileTemplate.js",
    "sb": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@hookform/resolvers": "^2.8.5",
    "@types/lodash.xor": "^4.5.6",
    "graphql": "^16.2.0",
    "lodash.xor": "^4.5.0",
    "next": "12.0.7",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.22.3",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/react": "^6.4.8",
    "@styled-icons/fa-brands": "^10.38.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-slick": "^0.23.7",
    "@types/styled-components": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^3.4.0",
    "apollo": "^2.33.9",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^7.12.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-plugin": "^4.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-storybook": "^0.5.3",
    "jest": "^27.4.3",
    "jest-styled-components": "^7.0.2",
    "plop": "^2.7.4",
    "prettier": "2.5.1",
    "typescript": "4.5.2"
  }
}
